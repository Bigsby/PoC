{
  "setup": {
    "js": [
      {
        "type": "install",
        "program": "Node.js",
        "link": "https://nodejs.org/",
        "notes": [
          "Make sure node.exe path is in System PATH"
        ]
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "node filename.js",
        "notes": [
          "If file extension is <code>.js</code> it can be omitted"
        ]
      }
    ],
    "cpp": [
      {
        "type": "install",
        "program": "Visual C++ or Visual Studio Community/Professional/Enterprise",
        "link": "https://www.visualstudio.com/vs/cplusplus/#downloadvs",
        "alternative": "https://www.visualstudio.com/downloads/",
        "notes": [
          "If installing Community/Professional/Enterprise, make sure to enable Features > Programming Languages > Visual C++ > Common Tools for Visual C++."
        ]
      },
      {
        "type": "open",
        "name": "Visual Studio > Developmer Command Prompt",
        "program": "VsDevCmd.bat"
      },
      {
        "type": "command",
        "text": "Compile code",
        "command": "cl /EHsc filename.cpp",
        "notes": [
          "<code>/EHsc</code> command-line option instructs the compiler to enable C++ exception handling."
        ]
      },
      {
        "type": "command",
        "text": "Run compiled program",
        "command": "filename.exe"
      }
    ],
    "py": [
      {
        "type": "install",
        "program": "Python",
        "link": "https://www.python.org/downloads/"
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "py filename.py"
      }
    ],
    "fs": [
      {
        "type": "install",
        "program": "Visual Studio Community/Professional/Enterprise",
        "link": "https://www.visualstudio.com/downloads/",
        "notes": [
          "Make sure to enable Features > Programming Languages > Visual F#"
        ]
      },
      {
        "type": "open",
        "name": "Visual Studio > Developmer Command Prompt",
        "program": "VsDevCmd.bat"
      },
      {
        "type": "command",
        "text": "Compile code",
        "command": "fsc [filename1.fs filename2.fs] filename.fs",
        "notes": [
          "The file containing the applicaiton entrypoint has to go in last."
        ]
      },
      {
        "type": "command",
        "text": "Run compiled program",
        "command": "filename.exe"
      }
    ],
    "go": [
      {
        "type": "install",
        "program": "Go",
        "link": "https://golang.org/dl/"
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "go run filename.go"
      }
    ],
    "rb": [
      {
        "type": "install",
        "program": "Ruby",
        "link": "https://rubyinstaller.org/"
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "ruby filename.rb"
      }
    ],
    "cs": [
      {
        "type": "install",
        "program": "Visual Studio Community/Professional/Enterprise",
        "link": "https://www.visualstudio.com/downloads/"
      },
      {
        "type": "open",
        "name": "Visual Studio > Developmer Command Prompt",
        "program": "VsDevCmd.bat"
      },
      {
        "type": "command",
        "text": "Compile code",
        "command": "csc [filename1.cs filename2.cs] filename.cs",
        "notes": [
          "The output file will be named after the file that contains the application entrypoint."
        ]
      },
      {
        "type": "command",
        "text": "Run compiled program",
        "command": "filename.exe"
      }
    ],
    "java": [
      {
        "type": "install",
        "program": "Java SE Development Kit",
        "link": "http://www.oracle.com/technetwork/java/javase/downloads/jdk8-downloads-2133151.html",
        "notes": [
          "Make sure java.exe and javac.exe paths are in System PATH"
        ]
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Compile code",
        "command": "javac [filename1.java filename2.java] filename.java"
      },
      {
        "type": "command",
        "text": "Run compiled program",
        "command": "java filename",
        "notes":[
          "Must be the filename containing the application entrypoint",
          "Must not containe file extension (compile result <code>.class</code>)"
        ]
      }
    ],
    "pl": [
      {
        "type": "install",
        "program": "Perl",
        "link": "http://dwimperl.com/windows.html",
        "alternative": "https://www.perl.org/get.html#win32"
      },
      {
        "type": "open",
        "name": "Command Prompt",
        "program": "cmd.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "perl filename.pl"
      }
    ],
    "ps1": [
      {
        "type": "install",
        "program": "PwerShell",
        "link": "https://github.com/PowerShell/PowerShell"
      },
      {
        "type": "open",
        "name": "Windows PowerShell",
        "program": "powershell.exe"
      },
      {
        "type": "command",
        "text": "Run program",
        "command": ".\\filename.ps1"
      }
    ],
    "php": [
      {
        "type": "download",
        "program": "PHP",
        "link": "http://windows.php.net/download/"
      },
      {
        "type": "non",
        "title": "UnZip",
        "text": "UnZip the file to a folder on your file system.",
        "notes": [
          "Make sure php.exe is in System PATH"
        ]
      },
      {
        "type": "command",
        "text": "Run program",
        "command": "php filename.php"
      }
    ],
    "vb": [
      {
        "type": "install",
        "program": "Visual Studio Community/Professional/Enterprise",
        "link": "https://www.visualstudio.com/downloads/"
      },
      {
        "type": "open",
        "name": "Visual Studio > Developmer Command Prompt",
        "program": "VsDevCmd.bat"
      },
      {
        "type": "command",
        "text": "Compile code",
        "command": "vbc filename.vb [filename1.vb filename2.vb]",
        "notes":[
          "The first file name will be used as the output <code>.exe</code> file."
        ]
      },
      {
        "type": "command",
        "text": "Run compiled program",
        "command": "filename.exe"
      }
    ]
  }
}